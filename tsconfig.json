{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ES2020",  // ou "ESNext", "CommonJS", etc.
    "target": "ES2020",  // ou "ESNext" dependendo da versão que você deseja
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
